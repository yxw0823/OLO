	<insert id="insertByBatch" parameterType="java.util.List"
		useGeneratedKeys="false">
		insert into "OLO_PD_PRODUCT"(
		"SHORT_TITLE",
		"DIMENSION",
		"LABEL",
		"CODE",
		"FLOOR_PRICE",
		"COMPANY",
		"PRICE",
		"COLOUR",
		"CATEGORY",
		"EFFECTIVE_TIME",
		"SEALED",
		"DETAIL",
		"CREATE_TIME",
		"CREATION_PEOPLE_ID",
		"TITLE",
		"SORTING",
		"REMARKS",
		"GOODS_ID"
		)
		<foreach collection="list" item="item" index="index"
			separator="union all">
			( SELECT #{item.SHORT_TITLE},
			#{item.DIMENSION},
			#{item.LABEL},
			#{item.CODE},
			#{item.FLOOR_PRICE},
			#{item.COMPANY},
			#{item.PRICE},
			#{item.COLOUR},
			#{item.CATEGORY},
			#{item.EFFECTIVE_TIME,jdbcType=TIMESTAMP},
			#{item.SEALED},
			#{item.DETAIL},
			#{item.CREATE_TIME,jdbcType=TIMESTAMP},
			#{item.CREATION_PEOPLE_ID,jdbcType=VARCHAR},
			#{item.TITLE},
			#{item.SORTING},
			#{item.REMARKS},
			#{item.GOODS_ID} FROM DUAL)
		</foreach>
	</insert>
