	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultMap="resultjcm">
		select
		a."SHORT_TITLE",
		a."SUB_TITLE",
		a."BRAND_NAME",
		a."LABEL",
		a."DIMENSION",
		a."CODE",
		a."FLOOR_PRICE",
		a."PRICE",
		a."COLOUR",
		a."COMPANY" ,
		a."STORE",
		a."CATEGORY",
		a."SUGGEST_PRICE",
		a."IMAGE_URL",
		a."STORAGE",
		a."LAST_DATE",
		a."EFFECTIVE_TIME",
		a."SEALED",
		to_char(	A.CREATE_TIME,'yyyy-mm-dd hh24:mi:ss') CREATE_TIME ,
		a."CREATION_PEOPLE_ID",
		to_char(	A.UPDATE_TIME,'yyyy-mm-dd hh24:mi:ss') UPDATE_TIME  ,
		a."UPDATE_PEOPLE_ID",
		a."SPREAD1",
		a."SPREAD2",
		a."SPREAD3",
		a."SPREAD4",
		a."SPREAD5",
		a."IMAGE_URL_LIST",
		a."REMARKS",
		a."TITLE",
		a."SORTING",
		a."GOODS_ID"
		from
		"OLO_PD_PRODUCT" a

		where 1=1
		<if test="pd.key!=null and pd.key!=''">
			and (a.CODE like CONCAT(CONCAT('%',#{pd.key}),'%')
			or a.TITLE like CONCAT(CONCAT('%',#{pd.key}),'%')
			or a.SUB_TITLE like CONCAT(CONCAT('%',#{pd.key}),'%')
			or a.CATEGORY like CONCAT(CONCAT('%',#{pd.key}),'%')
			)
		</if>
		<if test="pd.CODE!=null and pd.CODE!=''">
			and a.CODE like CONCAT(CONCAT('%',#{pd.CODE}),'%')
		</if>
		<if test="pd.TITLE!=null and pd.TITLE!=''">
			and a.TITLE like CONCAT(CONCAT('%',#{pd.TITLE}),'%')
		</if>
		<if test="pd.SUB_TITLE!=null and pd.SUB_TITLE!=''">
			and a.SUB_TITLE like CONCAT(CONCAT('%',#{pd.SUB_TITLE}),'%')
		</if>
		<if test="pd.classify!=null">
			and
			GOODS_ID IN (
			SELECT GOODS_ID from (SELECT GOODS_ID,wm_concat(T_ID) T_ID from
			OLO_PD_MENU_PRODUCT GROUP BY GOODS_ID) opmp
			WHERE
			<foreach item="item" index="index" collection="pd.classify" separator="OR">
				opmp.T_ID  LIKE     CONCAT(CONCAT('%','${item}'),'%')  
			</foreach>
			)
		</if>
		<if test="pd.SEALED!=null and pd.SEALED!=''">
			and a.SEALED = #{pd.SEALED}
		</if>
	</select>

	<!-- 列表 -->
	<select id="findlist" parameterType="pd" resultMap="resultjcm">
		select
		DISTINCT
		a."SHORT_TITLE",
		a."SUB_TITLE",
		a."LABEL",
		sysDic.NAME LABELName,
		sysDic.PATH LABELPath,
		a."DIMENSION",
		a."CODE",
		a."FLOOR_PRICE",
		a."PRICE",
		a."COLOUR",
		a."COMPANY" ,
		a."STORE",
		a."CATEGORY",
		a."STORAGE",
		a."LAST_DATE",
		a."EFFECTIVE_TIME",
		a."SEALED",
		a."TITLE",
		a."REMARKS",
		a."IMAGE_URL_LIST",
		a."SORTING",
		a."GOODS_ID"
		from
		"OLO_PD_PRODUCT" a
		LEFT JOIN OLO_PD_MENU_PRODUCT opmp ON opmp.GOODS_ID = a.GOODS_ID
		LEFT JOIN  SYS_DICTIONARIES  sysDic on sysDic.ZD_ID =  a.LABEL
		where 1=1

		<if test="key!=null and key!=''">
			and (a.CODE like CONCAT(CONCAT('%',#{key}),'%')
			or a.TITLE like CONCAT(CONCAT('%',#{key}),'%')
			or a.SUB_TITLE like CONCAT(CONCAT('%',#{key}),'%')
			)
		</if>
		<if test="CODE!=null and CODE!=''">
			and a.CODE like CONCAT(CONCAT('%',#{CODE}),'%')
		</if>
		<if test="TITLE!=null and TITLE!=''">
			and a.TITLE like CONCAT(CONCAT('%',#{TITLE}),'%')
		</if>
		<if test="SUB_TITLE!=null and SUB_TITLE!=''">
			and a.SUB_TITLE like CONCAT(CONCAT('%',#{SUB_TITLE}),'%')
		</if>
		<if test="classify!=null">
			and
			OPMP.T_ID
			in
			<foreach collection="classify" item="item" index="index"
				open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>

		<if test="SEALED!=null and SEALED!=''">
			and a.SEALED = #{SEALED}
		</if>
		and EFFECTIVE_TIME &lt; trunc(sysdate, 'mi')
		
		<if test="sort !=null and sort !=''">
				ORDER BY  ${sort}
		</if>
	</select>

