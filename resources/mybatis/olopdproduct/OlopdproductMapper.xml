<!-- 通过商品Id和关联类型，查询出关联的商品 -->
<select id="findTjspById" parameterType="List" resultMap="resultjcm">
	SELECT
		"CODE",
		"TITLE",
		"SHORT_TITLE",
		"SORTING",
		"PRICE",
		"CATEGORY",
		"REMARKS",
		"IMAGE_URL_LIST",
		"GOODS_ID"
	FROM
		OLO_PD_PRODUCT opp
	WHERE
		OPP.GOODS_ID IN (
			SELECT
				O_ID2
			FROM
				OLO_PD_AB_RELATION
			WHERE
				"TYPE" ='7'
			AND O_ID1 = #{GOODS_ID}
		)
		ORDER BY "SORTING"
	
</select>
	<!-- 通过GOODS_ID获取数据 -->
	<select id="findById" parameterType="pd" resultMap="resultjcm">
		select
		"SHORT_TITLE",
		"SUB_TITLE",
		"LABEL",
		"BRAND_NAME",
		"DIMENSION",
		"CODE",
		"COMPANY" ,
		"FLOOR_PRICE",
		"PRICE",
		"COLOUR",
		"STORE",
		"CATEGORY",
		"SUGGEST_PRICE",
		"IMAGE_URL",
		"STORAGE",
		"LAST_DATE",
		"EFFECTIVE_TIME",
		"SEALED",
		"DETAIL",
		to_char(	CREATE_TIME,'yyyy-mm-dd hh24:mi:ss') CREATE_TIME ,
		"CREATION_PEOPLE_ID",
		to_char(	UPDATE_TIME,'yyyy-mm-dd hh24:mi:ss') UPDATE_TIME ,
		"UPDATE_PEOPLE_ID",
		"SPREAD1",
		"SPREAD2",
		"SPREAD3",
		"SPREAD4",
		"SPREAD5",
		"IMAGE_URL_LIST",
		"TITLE",
		"SORTING",
		"REMARKS",
		"GOODS_ID"
		from
		"OLO_PD_PRODUCT"
		where
		1=1
		<if test="CODE!=null and CODE!=''">
			and CODE = #{CODE}
		</if>
		<if test="GOODS_ID!=null and GOODS_ID!=''">
			and "GOODS_ID" = #{GOODS_ID}
		</if>
	</select>
	<!-- 通过GOODS_ID获取数据 -->
	<select id="findImgsById" parameterType="pd" resultMap="resultjcm">
		SELECT
		"SRC",
		"SORTING",
		"LOCATION",
		"SEALED",
		 to_char(	CREATE_TIME,'yyyy-mm-dd hh24:mi:ss') CREATE_TIME ,
		"CREATION_PEOPLE_ID",
		"SPREAD1",
		"SPREAD2",
		"ID"
		from OLO_PD_CAROUSEL where ID in
		(SELECT O_ID2 from OLO_PD_AB_RELATION where
		O_ID1=#{GOODS_ID} AND TYPE =1) ORDER BY "LOCATION"  , "SORTING"
		
		
	</select>
