		a."GOODS_ID"
		from
		"OLO_PD_PRODUCT" a

		<if test="key!=null and key!=''">
			and (a.CODE like CONCAT(CONCAT('%',#{key}),'%')
			or a.TITLE like CONCAT(CONCAT('%',#{key}),'%')
			or a.SUB_TITLE like CONCAT(CONCAT('%',#{key}),'%')
			)
		</if>
		<if test="CODE!=null and CODE!=''">
			and a.CODE like CONCAT(CONCAT('%',#{CODE}),'%')
		</if>
		<if test="TITLE!=null and TITLE!=''">
			and a.TITLE like CONCAT(CONCAT('%',#{TITLE}),'%')
		</if>
		<if test="SUB_TITLE!=null and SUB_TITLE!=''">
			and a.SUB_TITLE like CONCAT(CONCAT('%',#{SUB_TITLE}),'%')
		</if>
		<if test="classify!=null">
			and
			GOODS_ID IN (

			SELECT GOODS_ID from (SELECT GOODS_ID,wm_concat(T_ID) T_ID from
			OLO_PD_MENU_PRODUCT GROUP BY GOODS_ID) opmp
			WHERE
			opmp.T_ID like replace(CONCAT(CONCAT('%', '
			<foreach item="item" index="index" collection="classify"
				open="" separator="," close="">${item}</foreach>
			'),'%'),' ','')
			)
		</if>
		<if test="SEALED!=null and SEALED!=''">
			and a.SEALED = #{SEALED}
		</if>
	</select>

	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from "OLO_PD_PRODUCT"
		where
		"GOODS_ID" in
		<foreach item="item" index="index" collection="array" open="("
			separator="," close=")">
			#{item}
		</foreach>
	</delete>

</mapper>