	<!-- 通过SUBJECT_ID获取数据 -->
	<select id="findById" parameterType="pd" resultMap="resultjcm">
		select 
			SD."NAME" TYPENAME,
			"TITLE",
			TYPE,	
			"AUTHOR",	
			"URL",	
			"BACKGROUND_IMG",	
			"CONTENT",	
			"SEO_KEYWORD",	
			"SEO_DESC",	
			"IS_SHOW",	
			"DELFLAG",	
			"CREATE_USER_ID",	
			"CREATE_DATE",	
			"UPDATE_USER_ID",	
			"UPDATE_DATE",	
			"TEMP1",	
			"TEMP2",	
			"TEMP3",	
			"TEMP4",	
			"TEMP5",	
			"MOBILE_CONTENT",	
			"MOBILE_SORT",	
			"MOBILE_TITLE_IMG",	
			"RELEASE_TIME",
			"SORT",	
			"SUBJECT_ID"
		from 
			"NP_INFORMATION_SUBJECT"  NIS
				LEFT JOIN SYS_DICTIONARIES sd ON SD.P_BM = NIS."TYPE"
		where 
			"SUBJECT_ID" = #{SUBJECT_ID}
	</select>
	

	<!-- 列表 -->
