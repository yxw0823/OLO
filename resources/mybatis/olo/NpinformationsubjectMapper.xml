				a."TEMP5",	
				a."MOBILE_SORT",	
				a."MOBILE_TITLE_IMG",
				a."SORT",	
				a."SUBJECT_ID"
		from 
				"NP_INFORMATION_SUBJECT" a
				LEFT JOIN SYS_DICTIONARIES sd ON SD.P_BM = a."TYPE"
				where 1=1 
				<if test="pd.TYPE !=null and  pd.TYPE !=''">
				  and 	a.TYPE = #{pd.TYPE}
				 	 <if test=" pd.TYPE =='0'">
				  		 and 	a.IS_SHOW ='1'
					</if>
				</if>
				<if test="pd.TITLE !=null and  pd.TITLE !=''">
				 	 and 	a.TITLE like  CONCAT(CONCAT('%',#{pd.TITLE}),'%')
				</if>
				
				
	</select>
	
